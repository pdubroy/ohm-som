ClassLoadingTest = TestCase (
  testEqualityOfClasses = (
    | a b c |
    b := ClassB new.
    a := ClassA new.
    c := ClassC new.
    
    self assert: 42 equals: b result.
    self assert: 42 equals: c result.
    
    self assert: a class equals: b class superclass.
    self assert: b class equals: c class superclass.
  )
)
